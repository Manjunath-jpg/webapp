pipeline{
    agent any
    stages{
        stage('git checkout'){
            steps{
                checkout scmGit(branches: [[name: '*/main']], extensions: [], userRemoteConfigs: [[credentialsId: 'git', url: 'https://github.com/Manjunath-jpg/webapp.git']])
            }
        }
        stage("build"){
            steps{
                sh 'mvn clean package'
            }
        }
        stage('test'){
            steps{
                withSonarQubeEnv('sonar-server-10.6'){
                    sh 'mvn sonar:sonar'
                   
                }
            }
        }
    }
}
